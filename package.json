{
  "name": "realtime-collaborative-code-editor",
  "version": "1.0.0",
  "description": "Real-time collaborative code editor with Monaco Editor and Socket.IO",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && nodemon index.js",
    "client:dev": "cd client && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "collaborative",
    "code-editor",
    "realtime",
    "socket.io",
    "monaco-editor"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}



